
.swiper-tab{
	width: 100%;
	border-top: 2rpx solid #777777;
	text-align: center;

	position:fixed;
	bottom: 0px;
	left:0px;
}
.swiper-tab-list{  
	font-size: 30rpx;
  display: inline-block;
	width: 33.33%;
	color: #777777;
}
.on{ color: #da7c0c;
	border-bottom: 5rpx solid #da7c0c;}
 
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }
.swiper-box view{
  text-align: center;
}
::-webkit-scrollbar{
	display:none;
}




